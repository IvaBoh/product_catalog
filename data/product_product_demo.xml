<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">

    <record id="game_chair" model="product.product"
            forcecreate="True">
        <field name="name">Game Chair</field>
        <field name="description">Nice game chair</field>
        <field name="price">1000</field>
        <field name="image_1920" type="base64"
               file="product_catalog/static/img/game_chair_1920.png"/>
        <field name="image_256" type="base64"
               file="product_catalog/static/img/game_chair_256.png"/>
        <field name="category_id" ref="product_category_gaming_chair"/>
        <field name="attribute_value_ids" eval="[Command.set([ref('product_attr_value_6'), ref('product_attr_value_2')])]"/>
    </record>

